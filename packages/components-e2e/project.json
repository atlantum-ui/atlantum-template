{
    "root": "packages/components-e2e",
    "sourceRoot": "packages/components-e2e/src",
    "projectType": "application",
    "targets": {
        "e2e": {
            "executor": "@nrwl/cypress:cypress",
            "options": {
                "cypressConfig": "packages/components-e2e/cypress.json",
                "devServerTarget": "components:storybook"
            },
            "configurations": {
                "ci": {
                    "devServerTarget": "components:storybook:ci"
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/components-e2e/**/*.{js,ts}"]
            }
        }
    },
    "tags": [],
    "implicitDependencies": ["components"]
}
